{
  "src": {
    "type": "file",
    "default": "https://storage.googleapis.com/orbix360-qa-plugins/defaults/images/change-me.png"
  },
  "width": {
    "type": "string",
    "default": "1"
  },
  "height": {
    "type": "string",
    "default": "1"
  },
  "visible": {
    "type": "boolean",
    "default": true
  },
  "shader": {
    "type": "string",
    "field": "select",
    "options": [
      "flat",
      "standard"
    ],
    "default": "flat"
  },
  "opacity": {
    "type": "string",
    "default": "1.0"
  },
  "side": {
    "type": "string",
    "field": "select",
    "options": [
      "front",
      "back",
      "double"
    ],
    "default": "front"
  },
  "repeat": {
    "type": "string",
    "default": "1 1"
  },
  "additional_components": {
    "type": "string",
    "field": "textarea",
    "default": ""
  }
}

