{
  "src": {
    "type": "file",
    "default": "https://storage.googleapis.com/orbix360-qa-plugins/defaults/images/change-me.png"
  },
  "height": {
    "type": "string",
    "default": "1"
  },
  "theta_length": {
    "type": "string",
    "default": "90"
  },
  "theta_start": {
    "type": "string",
    "default": "135"
  },
  "radius": {
    "type": "string",
    "default": "0.707"
  },
  "visible": {
    "type": "boolean",
    "default": true
  },
  "shader": {
    "type": "string",
    "field": "select",
    "options": [
      "flat",
      "standard"
    ],
    "default": "flat"
  },
  "opacity": {
    "type": "string",
    "default": "1.0"
  },
  "side": {
    "type": "string",
    "field": "select",
    "options": [
      "front",
      "back",
      "double"
    ],
    "default": "double"
  },
  "repeat": {
    "type": "string",
    "default": "-1 1"
  },
  "additional_components": {
    "type": "string",
    "field": "textarea",
    "default": ""
  }
}