{
  "image": {
    "type": "file",
    "default": "https://storage.googleapis.com/orbix360-qa-plugins/defaults/images/stars.jpeg"
  },
  "color": {
    "type": "string",
    "default": "#FFF"
  },
  "radius": {
    "type": "string",
    "default": "1100"
  },
  "additional_components": {
    "type": "string",
    "field": "textarea",
    "default": ""
  },
  "visible": {
    "type": "boolean",
    "default": true
  },
  "shader": {
    "type": "string",
    "field": "select",
    "options": [
      "flat",
      "standard"
    ],
    "default": "flat"
  },
  "opacity": {
    "type": "string",
    "default": "1.0"
  },
  "side": {
    "type": "string",
    "field": "select",
    "options": [
      "front",
      "back",
      "double"
    ],
    "default": "double"
  },
  "repeat": {
    "type": "string",
    "default": "1 1"
  },  
  "defaultPosition": {
    "type": "default",
    "absolute": true,
    "default": {
      "x": 0,
      "y": 0,
      "z": 0
    }
  }
}

